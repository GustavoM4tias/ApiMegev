{
  "JwtSettings": {
    "SecretKey": "${JWT_SECRET}",
    "Issuer": "megev",
    "Audience": "megev_users",
    "ExpirationInMinutes": 120
  },
  "ConnectionStrings": {
    "DefaultConnection": "${DB_CONNECTION_STRING}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
